{
  "name": "@voxport/pds-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/**/*.ts --format esm --clean && tsc -p tsconfig.json --emitDeclarationOnly",
    "dev": "tsup src/index.ts --watch",
    "lint": "eslint .",
    "typecheck": "tsc -b",
    "test": "vitest run"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@atproto/api": "^0.13.35",
    "@atproto/lexicon": "^0.5.1",
    "@atproto/xrpc": "^0.7.5"
  }
}
