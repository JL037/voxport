{
  "name": "@voxport/lexicons",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "src", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean && tsc -p tsconfig.json --emitDeclarationOnly",
    "dev": "tsup src/index.ts --watch",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "exports": {
    ".": { "types": "./dist/index.d.ts", "import": "./dist/index.js" }
  },
  "sideEffects": false
}
